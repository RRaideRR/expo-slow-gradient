      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.raider.expogradient",
      "splash": {
        "image": "./assets/splash.png",
        "resizeMode": "native"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "f93e7d7f-a852-4a46-88a5-d03bef6d6e9b"
      }
    }
  }
